#include <bits/stdc++.h>
using namespace std;
#define ll long long
 
int main(){
ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#ifndef ONLINE_JUDGE
    freopen("input.txt","r",stdin);
    freopen("output.txt","w",stdout);
#endif
 
 
 
    ll n;
    ll  b = 0, a;
    cin >> n;
    for (int i = 0; i < n - 1; i++)
    {
        cin >> a;
        b += a;
    }
    cout << n * (n + 1) / 2 - b;
 
 
    return 0;
}
